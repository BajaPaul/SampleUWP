<Page
    x:Class="SampleUWP.S01"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Loaded="Page_Loaded">
    
    <!-- This is generally going to be the About Page and will be last in the set.
    Don't want to need to renumber it every time the page count changes. -->

    <Page.Resources>
        <!-- Enter page specific resources here -->
    </Page.Resources>

    <Grid>

        <StackPanel>
            <Viewbox MaxHeight="150" MaxWidth="150" Margin="4,20,4,4" HorizontalAlignment="Center" VerticalAlignment="Center">
                <SymbolIcon Symbol="Setting" Foreground="{ThemeResource SystemControlHighlightAccentBrush}"/>
            </Viewbox>
            <TextBlock Style="{ThemeResource HeaderTextBlockStyle}" Text="Settings" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8"/>
            <TextBox x:Name="s01TextBox" Style="{ThemeResource AutoSuggestBoxTextBoxStyle}" PlaceholderText="Does entered text stay here?" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8" TabIndex="101" />
            <TextBlock Style="{ThemeResource BodyTextBlockStyle}" Text="Page caching is disabled" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8,8,8,16"/>
        </StackPanel>

    </Grid>
</Page>
